<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title id="siteTitle">@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="shortcut icon" type="image/x-icon" href="~/img/favicon-32x32.png">
    <link type="image/png" sizes="192x192" rel="icon" href="~/img/android-chrome-192x192.png">
    <link type="image/png" sizes="512x512" rel="icon" href="~/img/android-chrome-512x512.png">
</head>
<body>
<div style="width: 97.5vw; height: 90vh; margin: auto auto;">
    @RenderBody()
</div>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/signalr.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>